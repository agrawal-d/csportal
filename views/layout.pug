  body
    // Image and text
    nav.navbar.navbar-expand-lg.navbar-dark.bg-primary
      a.navbar-brand(href='/')
        img.d-inline-block.align-top(src='img/logo.png', width='25', height='25', alt='')
        |     <b>CSBITS</b>
      button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbarNav', aria-controls='navbarNav', aria-expanded='false', aria-label='Toggle navigation')
        span.navbar-toggler-icon
      #navbarNav.collapse.navbar-collapse
        ul.navbar-nav
          li.nav-item
            a.nav-link(href='/')
              i.fa.fa-home
              | Home 
          li.nav-item
            a.nav-link(href='/materials')
              i.fa.fa-book
              | Solutions
          li.nav-item
            a.nav-link(href='#')
              i.fa.fa-pencil-ruler
              |Challenges
          li.nav-item
            a.nav-link(href='#')
              i.fa.fa-comments
              | Discussions

    //Modal       
    .modal.modalify.default-modal(tabindex='-1', role='dialog')
      .modal-dialog(role='document')
        .modal-content
          .modal-header
            h5.modal-title.default-modal-title
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              span(aria-hidden='true') Ã—
          .modal-body.default-modal-body
            p Awaiting Content

    //Ajaxify
    .ajaxify-loader
      | Awaiting content ...

    .container.main
      block content
  
